<% content_for(:footer) do %><%= link_to t('users.login'),login_path%><% end %>
<% content_for(:boxstyle) do %>ib<% end %>
<% title t('users.create') %>
 <%= boxed_error_messages_for @user %>
<% form_for @user,:html => { :multipart => true } do |f| %>
<p>
    <%= f.label :username %><br />
    <%= f.text_field :username %>
</p>
<%= render :partial => f%>
<%= f.submit t('users.create_submit') %></p>
  
  
<% end %>
